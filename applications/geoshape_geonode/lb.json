		{
			"name":"main",
			"vpc": {
				"vpc_name":"<%=$vpc_name %>",
<% if !$deploy_id then %>
				,
				"deploy_id":"<%= $deploy_id %>"
<% end %>
			},
			"cross_zone_unstickiness":true,
			"idle_timeout":1020,
			"connection_draining_timeout":300,
			"healthcheck": {
				"target":"HTTP:8080/",
				"unhealthy-threshold":3,
				"healthy-threshold":3
			},
			"lb_cookie_stickiness_policy": {
				"name": "Geoshape"
			},
			"add_firewall_rules": [
				{
					"rule_name":"geoshape_geonode_lb"
				}
			],
			"listeners": [
				{
					"lb-port": 80,
					"lb-protocol": "HTTP",
					"instance-port": 80,
					"instance-protocol": "HTTP"
				},
				{
					"lb-port": 8080,
					"lb-protocol": "HTTP",
					"instance-port": 8080,
					"instance-protocol": "HTTP"
				}
			]
		}
